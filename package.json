{
  "name": "pathniti",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "setup-db": "node scripts/setup-database.js",
    "populate-questions": "node scripts/populate-assessment-questions.js",
    "populate-career-data": "node scripts/populate-career-data.js",
    "setup-assessment": "npm run populate-questions && npm run populate-career-data",
    "setup-ai-system": "node scripts/setup-ai-system.js",
    "generate-icons": "node scripts/generate-icons.js",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "build:mobile": "CAPACITOR_BUILD=true next build && npm run copy-mobile-build",
    "copy-mobile-build": "mkdir -p mobile-build && cp -r .next/static mobile-build/ && cp -r public mobile-build/ && cp .next/server/pages-manifest.json mobile-build/ && cp .next/BUILD_ID mobile-build/",
    "sync:android": "npx cap sync android",
    "sync:ios": "npx cap sync ios",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "build:android": "npm run build:mobile && npm run sync:android",
    "build:ios": "npm run build:mobile && npm run sync:ios",
    "run:android": "npm run build:android && npx cap run android",
    "run:ios": "npm run build:ios && npx cap run ios",
    "mobile:build": "node scripts/build-mobile.js build",
    "mobile:sync": "node scripts/build-mobile.js sync",
    "mobile:open": "node scripts/build-mobile.js open",
    "mobile:run": "node scripts/build-mobile.js run"
  },
